{
  "name": "xpipe",
  "version": "0.1.0",
  "description": "A JupyterLab extension for rendering xpipeline files.",
  "author": "Xpress AI <eduardo@xpress.ai>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "scripts": {
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "build": "tsc",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "extension:install": "jupyter labextension install xpipe",
    "extension:uninstall": "jupyter labextension uninstall  xpipe",
    "extension:enable": "jupyter labextension enable xpipe",
    "extension:disable": "jupyter labextension disable xpipe"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^10.0.27",
    "@jupyterlab/docregistry": "^3.1.8",
    "@jupyterlab/rendermime-interfaces": "^3.0.0",
    "@jupyterlab/ui-components": "^3.1.8",
    "@lumino/widgets": "^1.5.0",
    "@projectstorm/geometry": "^6.3.0",
    "@projectstorm/react-canvas-core": "^6.3.0",
    "@projectstorm/react-diagrams": "^6.3.0",
    "@projectstorm/react-diagrams-defaults": "^6.3.0",
    "@projectstorm/react-diagrams-routing": "^6.3.0",
    "axios": "^0.19.2",
    "closest": "0.0.1",
    "crypto": "^1.0.1",
    "dagre": "^0.8.5",
    "lodash": "^4.17.20",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "react-dnd": "^2.2.4", 
    "react-dnd-html5-backend": "^2.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/dagre": "^0.7.44",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "node": "^14.15.0",
    "rimraf": "^2.6.3",
    "typescript": "^4.1.3"
  }
}
